query fetch {
  customer: customerOrConsultant(id: "Q3VzdG9tZXIteDE=") {
    ... customer
    ... consultant
  }
}

fragment customer on Customer {
  name
  consultant {
    name
  }
  contracts {
    id
    ... on LifeInsuranceContract {
      premium
    }
    ... on SomeOtherContract {
      expiryDate
    }
  }
}

fragment consultant on Consultant {
  name
}
