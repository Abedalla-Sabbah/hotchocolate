public interface IFriend
{
    string Name { get; }
}

public class Friend
    : IFriend
{
    public string Name { get; set; }
}

public interface IHero
{
    string Name { get; }
    IReadOnlyList<Friend> Friends { get; }
}

public class Hero
    : IHero
{
    public string Name { get; set; }
    public List<Friend> Friends { get; set; }
    IReadOnlyList<Friend> IHero.Friends  => Friends;
}

